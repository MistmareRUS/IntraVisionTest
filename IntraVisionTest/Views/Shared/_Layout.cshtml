<!DOCTYPE html>
<html style="min-width:960px">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="background-color:antiquewhite; ">
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:coral">
        <div class="container">
            <div class="navbar-header">
                <h1 class="navbar-brand" style="color:saddlebrown">@ViewBag.Title</h1>
            </div>
        </div>
    </div>
    <div class="col-md-10 col-md-offset-1" style="top:80px;padding-bottom:130px">
        @RenderBody()
    </div>
    <div style="position:fixed; bottom:0px; height:120px; background-color:coral;border:solid; border-color:black; border-width:1px;width:100%">
        @RenderSection("cashbox", false)
    </div>

    <div style="position:fixed;top:82px;right:0px; max-width:8%; min-width:92px">
        @RenderSection("money", false)
    </div>


    <div style="position:fixed;top:82px;left:5px">
        @RenderSection("turn", false)
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryajax")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/MainHelper.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
